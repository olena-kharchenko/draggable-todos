, useEffect } from 'react';\nimport clamp from 'lodash-es/clamp';\nimport swap from 'lodash-move';\nimport { useGesture } from 'react-use-gesture';\nimport { useSprings, animated, interpolate } from 'react-spring';\nimport { RiDeleteBack2Line } from 'react-icons/ri';\nimport { BsArrowCounterclockwise } from 'react-icons/bs';\nimport { MdDone } from 'react-icons/md';\nimport s from './DraggableList.module.css';\n\n// Returns fitting styles for dragged/idle items\nconst fn = (order, down, originalIndex, curIndex, y) => index =>\n  down && index === originalIndex\n    ? {\n        y: curIndex * 100 + y,\n        scale: 1.1,\n        zIndex: '1',\n        shadow: 15,\n        immediate: n => n === 'y' || n === 'zIndex',\n      }\n    : {\n        y: order.indexOf(index) * 100,\n        scale: 1,\n        zIndex: '0',\n        shadow: 1,\n        immediate: false,\n      };\n\nexport default function DraggableList({\n  todos,\n  onToggleCompleted,\n  onDeleteTodo,\n}) {\n  const items = todos.map(item => item.text);\n  const order = useRef(items.map((_, index) => index)); // Store indicies as a local ref, this represents the item order\n  const [springs, setSprings] = useSprings(items.length, fn(order.current)); // Create springs, each corresponds to an item, controlling its transform, scale, etc.\n  const bind = useGesture(({ args: [originalIndex], down, delta: [, y] }) => {\n    const curIndex = order.current.indexOf(originalIndex);\n    const curRow = clamp(\n      Math.round((curIndex * 100 + y) / 100),\n      0,\n      items.length - 1,\n    );\n    const newOrder = swap(order.current, curIndex, curRow);\n    setSprings(fn(newOrder, down, originalIndex, curIndex, y)); // Feed springs new style data, they'll animate the view without causing a single render\n    if (!down) order.current = newOrder;\n  });\n\n  return (\n    <div className={s.content} style={{ height: items.length * 100 }}>\n      {springs.map(({ zIndex, shadow, y, scale }, i) => (\n        <div className={s.item}>\n          <animated.div\n            {...bind(i)}\n            key={i}\n            style={{\n              zIndex,\n              boxShadow: shadow.interpolate(\n                s => `rgba(0, 0, 0, 0.15) 0px ${s}px ${2 * s}px 0px`,\n              ),\n              transform: interpolate(\n                [y, scale],\n                (y, s) => `translate3d(0,${y}px,0) scale(${s})`,\n              ),\n            }}\n            children={\n              <>\n                <p className={todos[i].completed ? 'done' : null}>{items[i]}</p>\n                <button\n                  className={s.buttonDone}\n                  onClick={() => onToggleCompleted(todos[i].id)}\n                  aria-label=\"todo done\"\n                >\n                  {todos[i].completed ? (\n                    <BsArrowCounterclockwise />\n                  ) : (\n                    <MdDone />\n                  )}\n                </button>\n                <button\n                  className={s.buttonDelete}\n                  onClick={() => onDeleteTodo(todos[i].id)}\n                  aria-label=\"todo delete\"\n                >\n                  <RiDeleteBack2Line />\n                </button>\n              </>\n            }\n          />\n        </div>\n      ))}\n    </div>\n  );\n}\n","C:\\Users\\harch\\OneDrive\\Рабочий стол\\Лена\\React-goit\\draggable-todos\\src\\components\\ToDoList\\index.js",[],"C:\\Users\\harch\\OneDrive\\Рабочий стол\\Лена\\React-goit\\draggable-todos\\src\\components\\ToDoList\\ToDoList.js",[],["106","107"],"C:\\Users\\harch\\OneDrive\\Рабочий стол\\Лена\\React-goit\\draggable-todos\\src\\components\\TodoEditor\\index.js",[],"C:\\Users\\harch\\OneDrive\\Рабочий стол\\Лена\\React-goit\\draggable-todos\\src\\components\\TodoEditor\\TodoEditor.js",["108"],"import { useState } from 'react';\nimport { MdDone, MdClose } from 'react-icons/md';\nimport s from './TodoEditor.module.css';\n\nfunction TodoEditor({ onSubmit }) {\n  const [message, setMessage] = useState('');\n\n  const handleSubmit = e => {\n    e.preventDefault();\n    onSubmit(message);\n    setMessage('');\n  };\n\n  return (\n    <form className={s.TodoEditor} onSubmit={handleSubmit}>\n      <textarea\n        className={s.textarea}\n        value={message}\n        onChange={e => setMessage(e.currentTarget.value)}\n      ></textarea>\n\n      <button type=\"submit\" className={s.button} aria-label=\"save\">\n        <MdDone />\n      </button>\n    </form>\n  );\n}\n\nexport default TodoEditor;\n","C:\\Users\\harch\\OneDrive\\Рабочий стол\\Лена\\React-goit\\draggable-todos\\src\\components\\Modal\\index.js",[],"C:\\Users\\harch\\OneDrive\\Рабочий стол\\Лена\\React-goit\\draggable-todos\\src\\components\\Modal\\Modal.js",["109"],{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","severity":1,"message":"115","line":1,"column":25,"nodeType":"116","messageId":"117","endLine":1,"endColumn":34},{"ruleId":"110","replacedBy":"118"},{"ruleId":"112","replacedBy":"119"},{"ruleId":"114","severity":1,"message":"120","line":2,"column":18,"nodeType":"116","messageId":"117","endLine":2,"endColumn":25},{"ruleId":"114","severity":1,"message":"121","line":2,"column":10,"nodeType":"116","messageId":"117","endLine":2,"endColumn":18},"no-native-reassign",["122"],"no-negated-in-lhs",["123"],"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar",["122"],["123"],"'MdClose' is defined but never used.","'useState' is defined but never used.","no-global-assign","no-unsafe-negation"]